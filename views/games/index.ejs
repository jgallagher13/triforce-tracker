<%- include('../partials/header') %>

 <table>
   <thead>
     <tr>
       <th>Title</th>
       <th>Release Year</th>
     </tr>
   </thead>
   <tbody>
    <% games.forEach(function(g) { %>
       <tr>
         <td><%= g.title %></td>
         <td><%= g.releaseYear %></td>
         <img src="https://images.igdb.com/igdb/image/upload/t_cover_big/<%= imageId %>.jpg">
         <td id="game-info"><a href="/games/<%= g._id %>">Game Info</a></td>
       </tr>
       <% }); %>
   </tbody>
 </table>

 <%- include('../partials/footer') %>